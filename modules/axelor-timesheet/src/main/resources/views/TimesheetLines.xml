<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <grid model="com.axelor.project.db.TimesheetLines" title="TimesheetLines" name="TimesheetLines-grid" editable="true">
    <field name="project" />
    <field name="task" />
    <field name="dates" />
   <field name="activity"  selection="timesheet.timesheetlines.task.select"/>
    <field name="hours" />
    <field name="comments" />
  </grid>

  <form model="com.axelor.project.db.TimesheetLines" title="TimesheetLine" name="timesheet-line-form">
    <field name="project" />
    <field name="task"  />
    <field name="dates" />
    <field name="activity" selection="timesheet.timesheetlines.task.select"/>
    <field name="hours" />
    <field name="comments" />
  </form>



</object-views>
