<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">

  <module name="project" package="com.axelor.project.db" />

  <entity name="Timesheet" repository="abstract">


    <decimal name="hrs" title="hrs" />
    <string name="company" title="Company" default="Axelor India" />
    <decimal name="totalHrs" title="Hours" />
    <many-to-one name="user" column="user_id" ref="com.axelor.auth.db.User" title="User" />
    <date name="fromdate" title="From Date" />
    <date name="todate" title="To Date" />
    <one-to-many ref="com.axelor.project.db.TimesheetLines" name="timesheetLineList" title="TimesheetLine" mappedBy="timesheet" />
    <many-to-one ref="com.axelor.project.db.Project" name="projects" />
    <one-to-many ref="com.axelor.project.db.Activity" name="activityList" title="Activity" mappedBy="timesheet" />
    <many-to-one ref="com.axelor.meta.db.MetaFile" name="image" title="Profile" />



  </entity>

</domain-models>
